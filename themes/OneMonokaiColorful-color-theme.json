{
  "name": "One Monokai",
  "type": "dark",
  "colors": {
    "activityBar.background": "#222222",
    "activityBar.foreground": "#dcdcdc",
    "activityBarBadge.background": "#558b2f",
    "activityBarBadge.foreground": "#f3f3f3",
    "button.background": "#2e7d32",
    "diffEditor.insertedTextBackground": "#80808033",
    "dropdown.background": "#1f1f1f",
    "dropdown.border": "#1e1e1e",
    "editor.background": "#1E1E1E",
    "editor.foreground": "#cccccc",
    "editor.findMatchBackground": "#555555",
    "editor.findMatchHighlightBackground": "#444444",
    "editor.lineHighlightBackground": "#2c2c2c",
    "editor.selectionBackground": "#3e3e3e",
    "editorCursor.foreground": "#528bff",
    "editorError.foreground": "#c53f46",
    "editorGroup.emptyBackground": "#1e1e1e",
    "editorGroup.border": "#1e1e1e",
    "editorGroupHeader.tabsBackground": "#222222",
    "editorIndentGuide.background": "#454545",
    "editorHoverWidget.background": "#222222",
    "editorHoverWidget.border": "#1e1e1e",
    "editorLineNumber.foreground": "#555555",
    "editorRuler.foreground": "#484848",
    "editorSuggestWidget.background": "#222222",
    "editorSuggestWidget.border": "#1e1e1e",
    "editorSuggestWidget.selectedBackground": "#2c2c2c",
    "editorUnnecessaryCode.opacity": "#000000c0",
    "editorWhitespace.foreground": "#454545",
    "editorWidget.background": "#2c2c2c",
    "input.background": "#1d1d1d",
    "list.activeSelectionBackground": "#2c2c2c",
    "list.activeSelectionForeground": "#dedede",
    "list.focusBackground": "#3c3c3c",
    "list.highlightForeground": "#C5C5C5",
    "list.hoverBackground": "#333333",
    "list.inactiveSelectionBackground": "#2c2c2c",
    "list.inactiveSelectionForeground": "#d7d7d7",
    "notifications.background": "#252525",
    "scrollbarSlider.activeBackground": "#89898980",
    "scrollbarSlider.background": "#55555580",
    "scrollbarSlider.hoverBackground": "#5a5a5a80",
    "sideBar.background": "#1E1E1E",
    "sideBarSectionHeader.background": "#2c2c2c",
    "statusBar.background": "#222222",
    "statusBar.foreground": "#9d9d9d",
    "statusBarItem.hoverBackground": "#353535",
    "statusBar.noFolderBackground": "#222222",
    "statusBar.debuggingBackground": "#222222",
    "tab.activeBackground": "#454545",
    "tab.border": "#2a2a2a",
    "tab.inactiveBackground": "#2c2c2c",
    "terminal.foreground": "#ababab",
    "terminal.ansiBlack": "#313131",
    "terminal.ansiBlue": "#528bff",
    "terminal.ansiGreen": "#98c379",
    "terminal.ansiYellow": "#e5c07b",
    "terminal.ansiCyan": "#56b6c2",
    "terminal.ansiMagenta": "#c678dd",
    "terminal.ansiRed": "#e06c75",
    "terminal.ansiWhite": "#d7dae0",
    "terminal.ansiBrightBlack": "#7f848e",
    "terminal.ansiBrightBlue": "#528bff",
    "terminal.ansiBrightGreen": "#98c379",
    "terminal.ansiBrightYellow": "#e5c07b",
    "terminal.ansiBrightCyan": "#56b6c2",
    "terminal.ansiBrightMagenta": "#7e0097",
    "terminal.ansiBrightRed": "#f44747",
    "terminal.ansiBrightWhite": "#d7dae0",
    "titleBar.activeBackground": "#222222",
    "titleBar.activeForeground": "#9d9d9d",
    "titleBar.inactiveBackground": "#282828",
    "titleBar.inactiveForeground": "#6B6B6B",
    "peekViewEditor.background": "#222222",
    "menu.background": "#3c3c3c"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.template"
      ],
      "settings": {
        "foreground": "#dbc69aee"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ce93d8"
      }
    },
    {
      "name": "Embedded String Begin/End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#ffab91"
      }
    },
    {
      "name": "Default Embedded Color",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.erb",
        "punctuation.section.embedded.end.erb",
        "source.elixir.embedded",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#ababab"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#b2dfdb"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#80cbc4"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#c56c72",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#c5796cee"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#c5796cee"
      }
    },
    {
			"name": "Control flow / Special keywords",
			"scope": [
				"keyword.control",
				"source.cpp keyword.operator.new",
				"keyword.operator.delete",
				"keyword.other.using",
				"keyword.other.operator",
				"entity.name.operator"
			],
			"settings": {
				"foreground": "#c5796cee"
			}
		},
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#c56c72ee"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
        "storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#6EB3C0ee"
      }
    },
    {
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#6EB3C0ee"
			}
		},
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#91BDEA"
      }
    },
    {
      "name": "Variable Object",
      "scope": [
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
        "variable.readwrite",
        "variable.readwrite.other.block",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#ffe082ee"
      }
    },
    {
      "name": "Variable Object Constant",
      "scope": [
        "variable.other.constant",
        "variable.other.constant.property",
      ],
      "settings": {
        "foreground": "#dcdcdc",
      }
    },
    {
      "name": "Variable Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
      ],
      "settings": {
        "foreground": "#ffe082ee"
      }
    },
    {
      "name": "Variable Object Constant Property",
      "scope": [
        "variable.other.constant.property",
      ],
      "settings": {
        "foreground": "#dcdcdc",
      }
    },
    {
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#ababab"
			}
		},
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#A0C180"
      }
    },
    {
      "name": "Package Import",
      "scope": [
        "storage.modifier.import",
        "storage.modifier.package"
      ],
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal"
			],
			"settings": {
				"foreground": "#A0C180"
			}
		},
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C79B6D"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#A0C180"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": [
        "function.support.builtin",
        "function.support.core"
      ],
      "settings": {
        "foreground": "#A0C180"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.class.js"
      ],
      "settings": {
        "foreground": "#c56c72"
      }
    },
    {
      "name": "Tag Class and ID",
      "scope": [
        "entity.name.tag.class",
        "entity.name.tag.id"
      ],
      "settings": {
        "foreground": "#6EB3C0"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#A0C180"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#92cfda"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.variable"
      ],
      "settings": {
        "foreground": "#6EB3C0"
      }
    },
    {
      "name": "Json Property",
      "scope": "support.dictionary.json",
      "settings": {
        "foreground": "#6EB3C0"
      }
    },
    {
      "name": "StyleSheet Property name",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#ababab"
      }
    },
    {
      "name": "CSS: Pseudo Attribute Names",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.scss",
        "entity.other.attribute-name.pseudo-class.less",
        "entity.other.attribute-name.pseudo-class.sass",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-element.scss",
        "entity.other.attribute-name.pseudo-element.less",
        "entity.other.attribute-name.pseudo-element.sass"
      ],
      "settings": {
        "foreground": "#6EB3C0"
      }
    },
    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#A0C180"
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#DABD80"
      }
    },
    {
      "name": "StyleSheet Unit",
      "scope": [
        "unit.css",
        "unit.scss",
        "unit.less",
        "unit.sass"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "StyleSheet Function",
      "scope": [
        "function.css",
        "function.scss",
        "function.less",
        "function.sass"
      ],
      "settings": {
        "foreground": "#6EB3C0"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        // "background": "#c678dd",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        // "background": "#56b6c2",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "JSON String",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#6EB3C0"
      }
    },
    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#6EB3C0"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#56b6c2A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Punctuation",
      "scope": [
        "punctuation.definition.italic.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#676f7d"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Markup: Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#c678dd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Raw",
      "scope": [
        "markup.raw.inline.markdown",
        "markup.raw.block.markdown"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Markup: List Punctuation",
      "scope": "punctuation.definition.list_item.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    }
  ]
}
